{
    "name": "saproto/saproto",
    "description": "S.A. Proto Web Application",
    "homepage": "https://www.proto.utwente.nl",
    "keywords": [
        "proto",
        "utwente",
        "association"
    ],
    "authors": [
        {
            "name": "Jonathan Juursema",
            "email": "jonathan@proto.utwente.nl",
            "homepage": "https://www.jonathanj.nl",
            "role": "developer"
        },
        {
            "name": "Thijs Dortmann",
            "email": "thzd@proto.utwente.nl",
            "homepage": "http://www.thzd.nl",
            "role": "developer"
        },
        {
            "name": "Jonathan Matarazzi",
            "email": "j.matarazzi@proto.utwente.nl",
            "homepage": "https://jonathanm.nl/",
            "role": "developer"
        },
        {
            "name": "Jesse Visser",
            "email": "j.visser-1@proto.utwente.nl",
            "homepage": "https://jesse-visser.nl",
            "role": "developer"
        }
    ],
    "support": {
        "email": "haveyoutriedturningitoffandonagain@proto.utwente.nl",
        "issues": "https://github.com/saproto/saproto/issues",
        "source": "https://github.com/saproto/saproto"
    },
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.4",
        "abcaeffchen/sephpa": "^3.0.2",
        "alaouy/youtube": "2.2.5",
        "ext-calendar": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "globalcitizen/php-iban": "4.2.3",
        "graham-campbell/markdown": "^16.0.0",
        "guzzlehttp/guzzle": "^7.5",
        "jwilsson/spotify-web-api-php": "^7.0.0",
        "laravel/framework": "^v12.16.0",
        "laravel/helpers": "^1.7.0",
        "laravel/passport": "^13.3.0",
        "league/commonmark": "^2.3",
        "league/oauth1-client": "^1.7",
        "milon/barcode": "^12.0",
        "mollie/laravel-mollie": "^3.0",
        "nesbot/carbon": "^3.9.1",
        "pragmarx/google2fa": "^9.0.0",
        "sentry/sentry-laravel": "^4.4.1",
        "spatie/laravel-csp": "^3.21",
        "spatie/laravel-permission": "^6",
        "spipu/html2pdf": "^5.2",
        "vinkla/hashids": "^13.0.0",
        "willvincent/feeds": "^2.4",
        "setasign/tfpdf": "^1.33",
        "inertiajs/inertia-laravel": "^2.0",
        "spatie/laravel-data": "^4.15",
        "spatie/laravel-typescript-transformer": "^2.5",
        "spatie/enum": "^3.13",
        "pusher/pusher-php-server": "^7.2",
        "ext-iconv": "*",
        "ext-zlib": "*",
        "intervention/image-laravel": "^1.3",
        "laravel/socialite": "^5.19",
        "socialiteproviders/saml2": "^4.7",
        "google/apiclient": "^2.18.0",
        "ably/ably-php": "^1.1",
        "anhskohbo/no-captcha": "^3.7",
        "spatie/laravel-medialibrary": "^11.13",
        "league/flysystem-sftp-v3": "^3.30",
        "laravel/wayfinder": "^0.1.12",
        "laravel-notification-channels/webpush": "^10.4",
        "laravel/pennant": "^1.18",
        "league/flysystem-aws-s3-v3": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.13",
        "itsgoingd/clockwork": "^5.1",
        "laravel/tinker": "^2.6",
        "larastan/larastan": "^3.4",
        "laravel/sail": "^1.16",
        "laravel/pint": "^1.10",
        "driftingly/rector-laravel": "^2.0.2",
        "mockery/mockery": "^1.6",
        "pestphp/pest-plugin-laravel": "^4.0.0",
        "beyondcode/expose": "^3.0",
        "pestphp/pest-plugin-browser": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Factories\\": "database/factories/",
            "Google\\": "vendor/google/apiclient/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup",
        "fix": [
            "./vendor/bin/rector process && ./vendor/bin/pint --parallel && npx eslint resources/ --fix && npx prettier --write resources/"
        ],
        "pint": [
            "./vendor/bin/pint --parallel"
        ],
        "rector": [
            "vendor/bin/rector process"
        ],
        "ide-helper": [
            "php artisan cache:clear",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "analyse": [
            "vendor/bin/phpstan analyse"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "google/apiclient-services": [
            "Drive",
            "Gmail",
            "Directory"
        ]
    }
}
