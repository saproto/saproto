      "role": "developer"
    }
  ],
  "support": {
    "email": "haveyoutriedturningitoffandonagain@proto.utwente.nl",
    "issues": "https://github.com/saproto/saproto/issues",
    "source": "https://github.com/saproto/saproto"
  },
  "type": "project",
  "require": {
    "aacotroneo/laravel-saml2": "2.1.*",
    "abcaeffchen/sephpa": "2.0.0-beta1",
    "alaouy/youtube": "2.1.*",
    "doctrine/dbal": "2.*",
    "ext-curl": "*",
    "ext-json": "*",
    "globalcitizen/php-iban": "2.6.*",
    "graham-campbell/markdown": "13.1.*",
    "biscolab/laravel-recaptcha": "4.2.*",
    "intervention/image": "2.*",
    "intervention/imagecache": "2.*",
    "jwilsson/spotify-web-api-php": "1.12.*",
    "laravel/framework": "^7.30",
    "laravel/helpers": "1.*",
    "laravelcollective/html": "6.*",
    "laravel/passport": "^9.4.0",
    "league/commonmark": "1.5.*",
    "league/oauth1-client": "1.7.*",
    "lisennk/laravel-slack-web-api": "0.1.*",
    "milon/barcode": "7.*",
    "mollie/laravel-mollie": "2.15.0",
    "nesbot/carbon": "2.*",
    "nickurt/laravel-pwned-passwords": "1.*",
    "nickurt/laravel-postcodeapi": "1.15.*",
    "pragmarx/google2fa": "0.7.*",
    "swiftmailer/swiftmailer": "6.0.*",
    "sentry/sentry-laravel": "^2.6",
    "vinkla/hashids": "^8.0",
    "willvincent/feeds": "2.*",
    "spatie/laravel-csp": "^2.6",
    "spatie/laravel-permission": "^4.2.0",
    "ext-calendar": "*",
    "solitweb/directadmin": "^3.0",
    "spipu/html2pdf": "^5.2",
    "guzzlehttp/guzzle": "^6.5.5"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.8.2",
    "symfony/css-selector": "~4.0",
    "symfony/dom-crawler": "~4.0",
    "fakerphp/faker": "^1.13",
    "friendsofphp/php-cs-fixer": "^3.0.0",
    "laravel/tinker": "^2.6",
    "nunomaduro/larastan": "^1.0",
    "itsgoingd/clockwork": "^5.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Proto\\": "app/"
    }
  },
  "scripts": {
    "fix": [
      "php-cs-fixer fix --verbose"
    ],
    "ide-helper": [
      "php artisan cache:clear",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ],
    "analyse": [
      "vendor/bin/phpstan analyse"
    ],
    "post-install-cmd": [
      "php artisan clear-compiled"
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
